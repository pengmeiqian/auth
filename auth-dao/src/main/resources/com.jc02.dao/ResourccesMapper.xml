<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jc02.dao.ResourccesDao">
    <select id="findAll" parameterType="int"
            resultType="Resourcces">
        select * from Resourcces
    </select>
    <select id="findById" parameterType="String" resultType="Resourcces">
        select * from Resourcces where rid=#{rid}
    </select>
    <insert id="insert" parameterType="Resourcces">
        insert into Resourcces values(#{rid},#{rname},#{resurl},#{resorderno},#{redsdes},#{createTime},#{updatetime},#{createMan},#{updateMan},#{parentID})
    </insert>
    <update id="update" parameterType="Resourcces">
        update Resourcces set rname=#{rname},resurl=#{resurl},resorderno=#{resorderno},redsdes=#{redsdes},parentID=#{parentID}
    </update>
    <delete id="delete" parameterType="String">
        delete from Resourcces where rid=#{rid}
    </delete>
</mapper>